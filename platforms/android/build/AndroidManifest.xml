<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.sharkable.sharkengine"
      android:versionCode="___SHARKENGINE_APP_VERSION_CODE___"
      android:versionName="___SHARKENGINE_APP_VERSION___">
    <application android:label="___SHARKENGINE_APP_NAME___" android:icon="@drawable/ic_launcher">
        <activity android:name="SharkengineActivity"
                  android:label="___SHARKENGINE_APP_NAME___"
                  android:screenOrientation="sensorPortrait"
                  android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.ads.AdActivity"
                  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-sdk android:minSdkVersion="10"/>
    <supports-screens android:xlargeScreens="true"/>
</manifest>
